{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "settings": {
        "import/resolver": {
            "node": {
              "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
          }
    },
    "rules": {
        "import/extensions": ["error", "never"],
        "import/no-dynamic-require": ["error", "never"],
        "no-console":"error"
    }
}
