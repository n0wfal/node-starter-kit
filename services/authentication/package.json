{
  "name": "starter-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -e ts --exec ts-node ./src/bin/www",
    "build": "tsc && npm run cp-env && npm run cp-config",
    "start": "node ./dist/bin/www",
    "cp-env": "cp -r .env ./dist",
    "cp-config": "cp -r ./src/config/config.json ./dist/config/",
    "clean": "rm -rf ./dist",
    "clean-build": "npm run clean && npm run build",
    "test": "NODE_ENV=test mocha -r ts-node/register test/*.ts"
  },
  "dependencies": {
    "-": "0.0.1",
    "@sentry/node": "^5.23.0",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.1.2",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@types/chai-http": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.3"
  }
}
